<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hagogamesdk.baserecyclerviewadapterhelper">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.hagogamesdk.baserecyclerviewadapterhelper.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.hagogamesdk.baserecyclerviewadapterhelper.PullToRefreshUseActivity" />
        <activity android:name="com.hagogamesdk.baserecyclerviewadapterhelper.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.hagogamesdk.baserecyclerviewadapterhelper.AnimationUseActivity" />
        <activity android:name="com.hagogamesdk.baserecyclerviewadapterhelper.HeaderAndFooterUseActivity" />
        <activity android:name="com.hagogamesdk.baserecyclerviewadapterhelper.SectionUseActivity" />
        <activity android:name="com.hagogamesdk.baserecyclerviewadapterhelper.MultipleItemUseActivity" />
        <activity android:name="com.hagogamesdk.baserecyclerviewadapterhelper.EmptyViewUseActivity" />
        <activity android:name="com.hagogamesdk.baserecyclerviewadapterhelper.ItemDragAndSwipeUseActivity" />
        <activity android:name="com.hagogamesdk.baserecyclerviewadapterhelper.ExpandableUseActivity" />
        <activity android:name="com.hagogamesdk.baserecyclerviewadapterhelper.DataBindingUseActivity" />
        <activity android:name="com.hagogamesdk.baserecyclerviewadapterhelper.ItemClickActivity"></activity>
        <activity android:name="com.hagogamesdk.baserecyclerviewadapterhelper.base.BaseActivity"></activity>
        <activity
            android:name="com.hagogamesdk.baserecyclerviewadapterhelper.HomeActivity"
            android:launchMode="singleTask"></activity>
        <activity android:name="com.hagogamesdk.baserecyclerviewadapterhelper.UpFetchUseActivity" />
    </application>

</manifest>